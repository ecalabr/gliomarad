{
  "data_dir": "/data/gliogenomics/gliomarad",
  "model_dir": "same",
  "overwrite": true,
  "random_state": 42,

  "data_prefix": ["T1_wmtbn", "T2_wmtbn", "FLAIR_wmtbn", "DWI_wmtbn", "SWI_wmtbn", "ASL_wmn", "DTI_eddy_FA_wmn", "DTI_eddy_MD_wmn"],
  "label_prefix": ["T1gad_wmtbn"],
  "mask_prefix": ["tumor_core"],
  "mask_dilate": [232, 232, 80],
  "filter_zero": 0.2,

  "dimension_mode": "3D",
  "data_plane": "ax",
  "train_dims": [80, 80, 80],
  "train_patch_overlap": [4, 4, 4],
  "infer_dims": [232, 232, 232],
  "infer_patch_overlap": [1, 1, 1],
  "augment_train_data": true,
  "label_interp": 1,
  "mask_weights": false,
  "metrics": ["mae"],

  "norm_data": false,
  "norm_labels": false,
  "norm_mode": "zscore",

  "model_name": "unet_3d_bneck2",
  "base_filters": 32,
  "output_filters": 1,
  "layer_layout": [3, 4, 5],
  "final_layer": "conv",
  "kernel_size": [3, 3, 3],
  "data_format": "channels_last",
  "activation": "leaky_relu",
  "mixed_precision": true,
  "dist_strat": "mirrored",

  "shuffle_size": 1600,
  "batch_size": 8,
  "num_threads": 6,
  "samples_per_epoch": 32000,
  "train_fract": 0.9,
  "learning_rate": 0.001,
  "learning_rate_decay": "constant",
  "optimizer": "adam",
  "loss": "MSE",
  "num_epochs": 25,
  "dropout_rate": 0.3
}