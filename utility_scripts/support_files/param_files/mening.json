{
"T1gad":{
	"or": [["(?i)(t1)", "(?i)(postcon)"], ["(?i)rssg", "(?i)sp", "(?i)steo"], ["(?i)spgr", "(?i)postcon"], ["(?i)bravo", "(?i)postcon"]],
	"not": ["(?i)without", "(?i)w/o", "(?i)(ref|rfmt)", "(?i)(c|t|l)-sp", "(?i)spine", "(?i)(track|tract|left|right|arcuate|optic|ifof|slf|radiation|motor|roi)"],
	"reg": "trans",
	"reg_target": "atlas",
	"bias": true
	},

"FLAIR":{
	"or": [["(?i)flair"]],
	"not": ["(?i)t1", "(?i)(ref|rfmt)", "(?i)(track|tract|left|right|arcuate|optic|ifof|slf|radiation|motor|roi)"],
	"reg": "affine",
	"reg_target": "T1gad",
	"bias": true
	},

"T1":	{
	"or": [["(?i)(t1|bravo|spgr)"]],
	"not": ["(?i)postcon", "(?i)perf", "(?i)flair", "\\+", "(?i) pg ", "(?i)(c|t|l)-sp", "(?i)spine", "(?i)^gad", "(?i)(ref|rfmt)", "(?i)rho"],
	"reg": "affine",
	"reg_target": "T1gad",
	"bias": true
	},

"T2": 	{
	"or": [["(?i)t2"], ["(?i)fse"]],
	"not": ["(?i)flair", "(?i)trace", "(?i)quant", "(?i)t1", "(?i)perf", "(?i)t2\\*", "(?i)(ref|rfmt)", "(?i)(track|tract|left|right|arcuate|optic|ifof|slf|radiation|motor|roi)"],
	"reg": "affine",
	"reg_target": "T1gad",
	"bias": true
	}
}
