{
"DWI": 	{
	"or": [["(?i)dwi", "(?i)trace"]],
	"not": ["(?i)(ref|rfmt)", "(?i)exp", "(?i)cor", "(?i)sag", "(?i)eadc", "(?i)apparent"],
	"reg": "diffeo",
	"reg_target": "atlas",
	"bias": false,
	"split_func": "split_dwi"
	},
"ADC": 	{
	"or": [["(?i)adc"], ["(?i)apparent", "(?i)diffusion"], ["(?i)avdc"], ["(?i)average", "(?i)dc"]],
	"not": ["(?i)exp", "(?i)cor", "(?i)sag", "(?i)eadc"],
	"reg": "DWI"
	}
}
